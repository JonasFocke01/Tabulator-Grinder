{
    "manifest_version": 2,
    "name": "Tabulator Grinder",
    "version": "1.0.1",
    "author": "Jonas Focke",
    "description": "Manages open tabs by closing forgotten ones and freeing system resources.",
    "homepage_url": "https://github.com/JonasFocke01/Tabulator-Grinder",

    "icons": {
      "48": "icons/table-columns-solid.png",
      "96": "icons/table-columns-solid copy.png"
    },
  
    "options_ui": {
      "page": "options.html"
    },
  
    "permissions": [
      "menus",
      "storage",
      "tabs"
    ],

    "background": {
      "scripts": [
        "main.js"
      ]
    },

    "browser_specific_settings": {
      "gecko": {
        "id": "jonas-focke@mailbox.org",
        "strict_min_version": "42.0"
      }
    }
    
  }
  