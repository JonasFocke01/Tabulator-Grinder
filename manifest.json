{
    "manifest_version": 2,
    "name": "Tabulator Grinder",
    "version": "1.0.0",
    "description": "Automatically discard or suspend tabs after a short amount of time to preserve system resources",
    "homepage_url": "https://github.com/JonasFocke01/tabmanager-for-firefox",

    "icons": {
      "48": "icons/manager.png"
    },
  
    "options_ui": {
      "page": "options.html"
    },
  
    "permissions": [
      "menus",
      "storage",
      "tabs"
    ],

    "background": {
      "scripts": [
        "main.js"
      ]
    }
  }
  