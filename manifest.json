{
    "manifest_version": 2,
    "name": "Tabulator Grinder",
    "version": "1.0.7",
    "version_name": "Angle Grinder",
    "author": "Jonas Focke",
    "description": "Manages open tabs by closing forgotten ones and freeing system resources.",
    "homepage_url": "https://github.com/JonasFocke01/Tabulator-Grinder",

  
    "options_ui": {
      "page": "options.html"
    },
  
    "permissions": [
      "menus",
      "storage",
      "tabs"
    ],

    "background": {
      "scripts": [
        "main.js"
      ]
    },

    "browser_specific_settings": {
      "gecko": {
        "id": "{e3d5a6b9-488e-4f91-8991-df80456ee466}",
        "strict_min_version": "42.0"
      }
    }
    
  }
  